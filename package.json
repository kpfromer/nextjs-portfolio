{
  "name": "portfolio",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "precommit": "lint-staged",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "commit": "git cz"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.6.1",
    "@mdx-js/react": "1.6.1",
    "@rebass/forms": "4.0.6",
    "@rebass/preset": "4.0.5",
    "gatsby": "2.21.24",
    "gatsby-background-image": "1.1.1",
    "gatsby-image": "2.4.3",
    "gatsby-plugin-emotion": "4.3.1",
    "gatsby-plugin-google-analytics": "2.3.1",
    "gatsby-plugin-mdx": "1.2.6",
    "gatsby-plugin-preact": "3.3.1",
    "gatsby-plugin-react-helmet": "3.3.1",
    "gatsby-plugin-robots-txt": "1.5.0",
    "gatsby-plugin-sharp": "2.6.2",
    "gatsby-plugin-sitemap": "2.4.2",
    "gatsby-remark-copy-linked-files": "2.3.2",
    "gatsby-remark-images": "3.3.2",
    "gatsby-remark-katex": "3.3.1",
    "gatsby-remark-prismjs": "3.5.1",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-responsive-iframe": "2.4.2",
    "gatsby-remark-smartypants": "2.3.1",
    "gatsby-source-filesystem": "2.3.1",
    "gatsby-transformer-remark": "2.8.7",
    "gatsby-transformer-sharp": "2.5.2",
    "katex": "0.11.1",
    "preact": "10.4.1",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.0.0",
    "react-icons": "3.10.0",
    "rebass": "4.0.7",
    "remark-html-katex": "2.0.1",
    "remark-math": "2.0.1",
    "surge": "0.21.3",
    "theme-ui": "0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "6.8.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.2",
    "prettier": "2.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kpfromer/portfolio"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
