{
  "name": "portfolio",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,mdx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "precommit": "lint-staged",
    "typecheck": "tsc --pretty",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "commit": "git cz"
  },
  "dependencies": {
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@mdx-js/mdx": "1.6.6",
    "@mdx-js/react": "1.6.6",
    "@rebass/forms": "4.0.6",
    "@rebass/preset": "4.0.5",
    "gatsby": "2.23.22",
    "gatsby-background-image": "1.1.1",
    "gatsby-image": "2.4.12",
    "gatsby-plugin-emotion": "4.3.9",
    "gatsby-plugin-google-analytics": "2.3.10",
    "gatsby-plugin-mdx": "1.2.22",
    "gatsby-plugin-preact": "4.0.8",
    "gatsby-plugin-react-helmet": "3.3.9",
    "gatsby-plugin-robots-txt": "1.5.1",
    "gatsby-plugin-sharp": "2.6.18",
    "gatsby-plugin-sitemap": "2.4.10",
    "gatsby-remark-copy-linked-files": "2.3.10",
    "gatsby-remark-images": "3.3.17",
    "gatsby-remark-katex": "3.3.9",
    "gatsby-remark-prismjs": "3.5.9",
    "gatsby-remark-reading-time": "1.1.0",
    "gatsby-remark-responsive-iframe": "2.4.10",
    "gatsby-remark-smartypants": "2.3.9",
    "gatsby-source-filesystem": "2.3.18",
    "gatsby-transformer-remark": "2.8.23",
    "gatsby-transformer-sharp": "2.5.10",
    "katex": "0.11.1",
    "preact": "10.4.5",
    "prismjs": "1.20.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-icons": "3.10.0",
    "rebass": "4.0.7",
    "remark-html-katex": "2.0.1",
    "remark-math": "2.0.1",
    "surge": "0.21.5",
    "theme-ui": "0.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@types/node": "14.0.19",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-helmet": "6.0.0",
    "@types/rebass": "4.0.6",
    "@types/rebass__forms": "4.0.2",
    "@types/theme-ui": "0.3.4",
    "@typescript-eslint/eslint-plugin": "3.6.0",
    "@typescript-eslint/parser": "3.6.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.20.3",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "typescript": "3.9.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kpfromer/portfolio"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn typecheck && yarn precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
